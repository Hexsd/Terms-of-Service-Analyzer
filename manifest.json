{
  "name": "Terms of Service Analyzer",
  "description": "Extract data from user agreement",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting","storage"],
  "host_permissions": ["http://localhost:1234/*"],
  "web_accessible_resources": [
    {
      "resources": ["patterns.json"],
      "matches": ["<all_urls>"]
    }
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}